package com.tomtom.security.shiro;

import org.apache.shiro.ShiroException;

/**
 * TomTom International BV Proprietary
 *
 * @author Ilya Pimenov
 */
public class MuidException extends ShiroException {

    public MuidException() {
        super();
    }

    public MuidException(String message) {
        super(message);
    }

    public MuidException(Throwable cause) {
        super(cause);
    }

    public MuidException(String message, Throwable cause) {
        super(message, cause);
    }
}
