<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{mm:ss.SSS} %-5level %logger{25} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="neo4j" level="OFF"/>

    <root level="debug">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>